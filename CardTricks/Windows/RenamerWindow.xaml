<Window x:Name="RenamerWindow1" x:Class="CardTricks.Windows.RenamerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename " Height="135.2" Width="300" ResizeMode="NoResize"
        
        >
    <Grid Background="#FF85A0B8">
        <TextBox x:Name="textboxName" Height="23" Margin="10,24,10,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="274"/>
        <Button x:Name="buttonOk" Content="Ok" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top" Width="75" Click="buttonOk_Click" IsDefault="True"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="90,67,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" Click="buttonCancel_Click" RenderTransformOrigin="-0.568,0.713"/>

    </Grid>
</Window>
