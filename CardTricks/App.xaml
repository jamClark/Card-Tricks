<Application x:Class="CardTricks.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             StartupUri="Windows/MainWindow.xaml"
             xmlns:cmd="clr-namespace:CardTricks.Cmds"
             xmlns:con="clr-namespace:CardTricks.Controls"
             >
    
    <Application.Resources>

        <ResourceDictionary >
            <TextBlock x:Key="CursorRotate" Visibility="Hidden" IsEnabled="False" Cursor="Resources/Cursors/RotateCursor.cur"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Menus.xaml" />
                <ResourceDictionary Source="Resources/DesignerItem.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

        


    </Application.Resources>
    
</Application>
